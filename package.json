{
  "name": "grammy-nestjs",
  "description": "Grammy module for NestJS",
  "version": "0.0.2",
  "author": {
    "name": "Mike Crowe",
    "email": "drmikecrowe@gmail.com"
  },
  "contributors": [
    {
      "name": "Arthur Asimov",
      "email": "arthur.asimov.z0@gmail.com"
    }
  ],
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "debug": "^4.3.4",
    "grammy": "^1.7.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0",
    "source-map-support": "^0.5.21",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@drmikecrowe/eslint-config-prettier-typescript": "^0.3.8",
    "@drmikecrowe/prettier-config": "*",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@nrwl/cli": "13.10.2",
    "@nrwl/devkit": "^12.1.1",
    "@nrwl/eslint-plugin-nx": "13.10.2",
    "@nrwl/jest": "13.10.2",
    "@nrwl/js": "13.10.2",
    "@nrwl/linter": "13.10.2",
    "@nrwl/nest": "^13.10.2",
    "@nrwl/node": "13.10.2",
    "@nrwl/workspace": "13.10.2",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "~5.18.0",
    "@typescript-eslint/parser": "~5.18.0",
    "eslint": "~8.12.0",
    "eslint-config-prettier": "8.1.0",
    "husky": "^7.0.0",
    "jest": "27.2.3",
    "lint-staged": "^12.3.8",
    "ngrok": "^4.3.1",
    "nx": "13.10.2",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.5.1",
    "syncpack": "^6.2.1",
    "ts-jest": "27.0.5",
    "typescript": "~4.6.2"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@drmikecrowe/eslint-config-prettier-typescript"
    ],
    "ignorePatterns": [
      "**/*"
    ],
    "plugins": [
      "@nrwl/nx"
    ],
    "overrides": [
      {
        "files": [
          "*.ts",
          "*.tsx",
          "*.js",
          "*.jsx"
        ],
        "rules": {
          "@nrwl/nx/enforce-module-boundaries": [
            "error",
            {
              "enforceBuildableLibDependency": true,
              "allow": [],
              "depConstraints": [
                {
                  "sourceTag": "*",
                  "onlyDependOnLibsWithTags": [
                    "*"
                  ]
                }
              ]
            }
          ]
        }
      },
      {
        "files": [
          "*.ts",
          "*.tsx"
        ],
        "extends": [
          "plugin:@nrwl/nx/typescript"
        ],
        "rules": {
          "@typescript-eslint/no-explicit-any": 0
        }
      },
      {
        "files": [
          "*.js",
          "*.jsx"
        ],
        "extends": [
          "plugin:@nrwl/nx/javascript"
        ],
        "rules": {}
      }
    ]
  },
  "keywords": [
    "bot",
    "bot api",
    "bot framework",
    "grammy",
    "nest",
    "nestjs",
    "nestjs-module",
    "telegram",
    "telegram bot",
    "telegram bot api"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,json,css,scss,md,ts,html,graphql}": [
      "nx format:write --uncommitted"
    ]
  },
  "prettier": "@drmikecrowe/prettier-config",
  "private": true,
  "scripts": {
    "format": "prettier --loglevel warn --write \"*.js,*.ts\"",
    "lint": "eslint . --cache --fix",
    "posttest": "npm run format",
    "prepare": "husky install && sed -i 's@#!/bin/sh@#!/usr/bin/env bash@g' .husky/_/husky.sh && sed -i 's@sh -e@bash -e@g' .husky/_/husky.sh",
    "pretest": "yarn lint"
  }
}
